CASE 
                WHEN s.interest = 0 AND s.payment = 0 THEN 'interest_payment'
                WHEN s.interest = 0 THEN 'interest_payment ' + CAST(s.payment AS VARCHAR)
                WHEN s.payment = 0 THEN 'interest ' + CAST(s.interest AS VARCHAR) + '_payment'
                ELSE 'interest ' + CAST(s.interest AS VARCHAR) + '_payment ' + CAST(s.payment AS VARCHAR)
            END,
